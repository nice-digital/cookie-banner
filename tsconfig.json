{
  "compilerOptions": {
    "target": "es5",
    "lib": ["es6", "DOM"],
    "module": "CommonJS",
    "rootDir": "src",
    "outDir": "lib",
    "sourceMap": true,
    "strict": true,
    "strictNullChecks": true,
    "esModuleInterop": true,
    "noImplicitAny": true,
    "forceConsistentCasingInFileNames": true,
    "allowJs": true,
    "declaration": true
  },
  "exclude": ["node_modules", "lib", "src/cdn.ts"],
  "include": ["src"]
}
