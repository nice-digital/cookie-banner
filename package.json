{
  "name": "@nice-digital/cookie-banner",
  "version": "1.0.0",
  "description": "Cookie banner and management for NICE digital services",
  "main": "lib/index.js",
  "scripts": {
    "// LOCAL DEV": "",
    "start": "cross-env NODE_ENV=development webpack-dev-server --hot",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.prod.js",
    "// TESTS": "",
    "test": "jest --notify",
    "test:coverage": "jest --notify --coverage",
    "test:watch": "jest --watch",
    "// LINTING/CODE STYLE": "",
    "lint": "npm run prettier && npm run lint:ts && npm run ts:check",
    "prettier": "prettier --check \"./**/*.{ts,tsx,js}\"",
    "prettier:fix": "npm run prettier -- --write",
    "lint:ts": "eslint \"**/*.{js,jsx,ts,tsx}\"",
    "lint:ts:fix": "npm run lint:ts -- --fix",
    "// TYPECHECKING": "",
    "ts": "tsc",
    "ts:check": "tsc --noEmit",
    "// NPM LIFECYCLE": "",
    "prepack": "npm run ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nice-digital/cookie-banner.git"
  },
  "keywords": [
    "cookie"
  ],
  "author": "NICE Digital (https://www.nice.org.uk/digital)",
  "contributors": [
    "NICE Digital Services (https://www.nice.org.uk/digital)",
    "Ian Routledge (https://www.ediblecode.com/)"
  ],
  "license": "MIT",
  "homepage": "https://github.com/nice-digital/cookie-banner#readme",
  "files": [
    "lib"
  ],
  "devDependencies": {
    "@nice-digital/nds-core": "^1.0.23-alpha.0",
    "@types/jest": "^26.0.10",
    "@types/js-cookie": "^2.2.6",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "@webpack-cli/serve": "^1.5.1",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "cross-env": "^7.0.2",
    "css-loader": "^6.0.0",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "html-webpack-plugin": "^5.3.2",
    "jest": "^26.4.2",
    "jest-teamcity-reporter": "^0.9.0",
    "prettier": "^2.0.5",
    "sass": "^1.26.11",
    "sass-loader": "^12.1.0",
    "style-loader": "^3.1.0",
    "ts-jest": "^26.3.0",
    "ts-loader": "^9.2.3",
    "typescript": "^3.9.7",
    "val-loader": "^4.0.0",
    "webpack": "^5.44.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^4.0.0-beta.3",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "js-cookie": "^2.2.1"
  }
